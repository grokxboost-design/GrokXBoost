# xAI API key from console.x.ai
XAI_API_KEY=your_xai_api_key_here

# Upstash Redis via Vercel (for report persistence & rate limiting)
# Get these from your Vercel project -> Storage -> Upstash Redis
# Or directly from upstash.com
KV_REST_API_URL=your_kv_rest_api_url_here
KV_REST_API_TOKEN=your_kv_rest_api_token_here

# Waitlist notifications (optional)
# Get API key from resend.com
RESEND_API_KEY=your_resend_api_key_here
RESEND_FROM_EMAIL=onboarding@resend.dev
WAITLIST_NOTIFY_EMAIL=your_email@example.com
